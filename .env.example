# Name of the app
APP_NAME="My Business"

# Full url to image, or leave empty for no logo
APP_LOGO="" 

# The url where the app is installed
APP_URL=http://localhost

# Public e-mail address visible on site (contact)
APP_CONTACT_EMAIL="hi@example.com"

# Environment, "production" is recommended
APP_ENV="production"

# Key is automatically generated during installation
APP_KEY=

# Whether to show full error messages (false recommended for production, true for testing) 
APP_DEBUG=true

# Default locales
DEFAULT_LANGUAGE="en"
DEFAULT_LOCALE="en_US"
DEFAULT_TIMEZONE="UTC"
DEFAULT_CURRENCY="USD"

# Default rules
DEFAULT_POINTS_EXPIRATION_MONTHS=""
DEFAULT_POINTS_PER_CURRENCY=""
DEFAULT_POINT_VALUE=""
DEFAULT_MIN_POINTS_PER_PURCHASE=""
DEFAULT_MAX_POINTS_PER_PURCHASE=""
DEFAULT_MIN_POINTS_PER_REDEMPTION=""
DEFAULT_MAX_POINTS_PER_REDEMPTION=""

# Premium pricing
PREMIUM_PRICE_MONTHLY=""
PREMIUM_PRICE_YEARLY=
PREMIUM_PRICE_CURRENCY=""

# Payment gateway
# ---
# Available options: "manual" or "stripe".
# If "manual" is set, a message will be displayed telling 
# a user to send an email to the public email address (APP_CONTACT_EMAIL).
PAYMENT_GATEWAY="manual"

# Stripe keys (required if PAYMENT_GATEWAY="stripe")
# See docs for detailed instructions on how to configure this.
# ---
# Developers > API keys > Publishable key (https://dashboard.stripe.com/test/apikeys)
STRIPE_PUBLIC_KEY=""
# Developers > API keys > Secret key (https://dashboard.stripe.com/test/apikeys)
STRIPE_SECRET_KEY=""

# Stripe plans (required if PAYMENT_GATEWAY="stripe")
# ---
# See docs for detailed instruction. Make sure the ID starts with "plan_".
# (e.g. plan_FbpVUHSriThQKv)
STRIPE_PLAN_ID_MONTHLY=""
STRIPE_PLAN_ID_YEARLY=""

# Social links in footer
# ---
# Enter the full url to your social account
# (e.g. https://twitter.com/NowSquareCom)
SOCIAL_TWITTER=""
SOCIAL_YOUTUBE=""
SOCIAL_FACEBOOK=""
SOCIAL_INSTAGRAM=""
SOCIAL_MEDIUM=""
SOCIAL_SNAPCHAT=""
SOCIAL_LINKEDIN=""
SOCIAL_GITHUB=""

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# Sender name and e-mail address 
MAIL_FROM_NAME="My Business"
MAIL_FROM_ADDRESS="noreply@example.com"

# Supported drivers: "sendmail", "smtp" and "mailgun"
# https://laravel.com/docs/6.x/mail
# For simple and reliable email we recommend https://sendgrid.com/
# When using sendgrid, use MAIL_DRIVER=smtp
MAIL_DRIVER=sendmail
MAIL_HOST=smtp.sendgrid.net
MAIL_PORT=465
MAIL_ENCRYPTION=ssl
MAIL_USERNAME=apikey
MAIL_PASSWORD=null

# Only when using Mailgun
# The values can be api.mailgun.net, api.eu.mailgun.net or api.us.mailgun.net
# depending on the region you selected in the Mailgun dashboard.
MAILGUN_ENDPOINT=api.mailgun.net
MAILGUN_DOMAIN=mg.mymailgundomain.com
MAILGUN_SECRET=

# Google Analytics code
# UA-xxxxxxxxx-x
CONFIG_GOOGLE_ANALYTICS=""

LOG_CHANNEL=stack
BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
